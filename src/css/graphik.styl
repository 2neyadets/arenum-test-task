@font-face
  font-family: 'Graphik'
  src: local("Graphik"),
          url('fonts/Graphik/GraphikLCG-Light.eot?') format("eot"),
          url('fonts/Graphik/GraphikLCG-Light.woff2') format('woff2'),
          url('fonts/Graphik/GraphikLCG-Light.ttf') format("truetype");
  font-weight: 200
  font-style: normal
